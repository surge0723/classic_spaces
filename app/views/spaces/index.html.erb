<div class="container">
        <table class="table table-hover table-inverse">
          <thead>
            <tr>
            <th>会場名</th>
            <th>場所</th>
            <th>詳細</th>
            </tr>
          </thead>
        <% @spaces.each do |space| %>
          <tbody>
            <tr>
              <td><%= space.space %></td>
              <td><%= space.place %></td>
              <td><%= link_to '詳細',space_path(space.id) %></td>
              <% end %>
            </tr>
          </tbody>
        </table>
</div>