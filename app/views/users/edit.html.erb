<h1>user_edit</h1>
<%= form_for @user do |f| %> 
 <div class="form-group">
              <label for="inputName">ユーザー名</label>
              <div><%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"name"%></div>
          </div>
          <div class="form-group">
          <label for="inputEmail">登録メールアドレス</label>
          <div><%= f.text_field :email, autofocus: true, id:"inputEmail", placeholder:"email"%></div>
          
  		  <%= f.submit "ユーザー情報を変更する",class: "btn btn-secondary"  %>
  		  <% end %>
     </div>
  </div>